{
    "manifest_version": 3,
    "name": "Saved Tab - New Tab Page",
    "version": "2.0",
    "description": "A custom new tab extension.",
    "permissions": ["storage", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icon-32.png",
            "128": "icon-128.png"
        }
    },
    "icons": {
        "32": "icon-32.png",
        "128": "icon-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "new tab page.html"
    },
    "default_locale": "en"
}
